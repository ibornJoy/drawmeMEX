{
    "author": "lenjin",
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "browser_action": {
        "default_title": "DrawMe",
        "default_icon": {
            "48": "icon.png"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "http://www.okooo.com/*"
            ],
            "all_frames": true,
            "js": [
                "content_script.js"
            ]
        }
    ],
    "description": "Draw HighChart from okkooo.com.",
    "icons": {
        "16": "icon48.png",
        "48": "icon48.png",
        "128": "icon48.png",
        "256": "icon48.png"
    },
    "manifest_version": 2,
    "name": "DrawMe",
    "permissions": [
        "tabs",
        "http://www.okooo.com/*"
    ],
    "version": "1.1",
    "-ms-preload": {
        "backgroundScript": "backgroundScriptsAPIBridge.js",
        "contentScript": "contentScriptsAPIBridge.js"
    }
}